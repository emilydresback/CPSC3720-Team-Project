# Production environment variables for deployed backend services

# Database
DATABASE_URL=./backend/shared-db/database.sqlite

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=30m

# Service Ports
AUTH_PORT=7005
ADMIN_PORT=5001
CLIENT_PORT=6001
LLM_PORT=5003

# CORS Settings
FRONTEND_URL=https://tigertix-frontend.vercel.app
CORS_ORIGIN=https://tigertix-frontend.vercel.app

# Node Environment
NODE_ENV=production